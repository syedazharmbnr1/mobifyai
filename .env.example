# Server configuration
PORT=3000
NODE_ENV=development

# Database configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/mobifyai
REDIS_URL=redis://localhost:6379

# LLM Service configuration
LLM_SERVICE_URL=http://localhost:3001
LLM_SERVICE_API_KEY=your_internal_api_key

# OpenAI configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORGANIZATION_ID=your_openai_org_id
OPENAI_DEFAULT_MODEL=gpt-4o

# Anthropic configuration
ANTHROPIC_API_KEY=your_anthropic_api_key
ANTHROPIC_DEFAULT_MODEL=claude-3-opus-20240229

# Google AI configuration
GOOGLE_AI_API_KEY=your_google_ai_api_key
GOOGLE_DEFAULT_MODEL=gemini-pro

# Cohere configuration
COHERE_API_KEY=your_cohere_api_key
COHERE_DEFAULT_MODEL=command

# Local model configuration
LOCAL_MODELS_DIR=/path/to/local/models
LOCAL_DEFAULT_MODEL=llama-3-8b-q4
LMSTUDIO_HOST=http://localhost:1234/v1
OLLAMA_HOST=http://localhost:11434/api

# Preview service configuration
PREVIEW_SERVICE_URL=http://localhost:3002
PREVIEW_SERVICE_API_KEY=your_internal_preview_api_key

# GitHub configuration for code repositories
GITHUB_TOKEN=your_github_token
GITHUB_USERNAME=your_github_username
GITHUB_ORGANIZATION=your_github_organization

# JWT configuration
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=8h
JWT_REFRESH_EXPIRATION=7d

# Logging configuration
LOG_LEVEL=info
